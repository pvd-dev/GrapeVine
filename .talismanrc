fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: de7e31e4ddf40cf235461f32edd9089ef82fad912879d2be810b4376d46931b3
  ignore_detectors: [filename] 
version: ""

scopeconfig:
