fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: 0885d37309a7a836b3f92d8a5662746f
  ignore_detectors: [filename] 
version: ""

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium

allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}    