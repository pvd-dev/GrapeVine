fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: b69f4ab9f4615a64a4c815713508b64e99c912ec4001ff6515b2ac42d2cc85b1
  ignore_detectors: [filename] 
version: ""

scopeconfig:
