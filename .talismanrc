fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: 0f4a28382fffdd929c643c681f262e5056eafc0023c40710d2fb64c924440250
  ignore_detectors: [filename] 
version: ""

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium

allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}    