fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: edc3a23285b1b1dd82b32280b28c493ef696f3bb8373deacad932dd70c24804e
  ignore_detectors: [filename] 
version: ""

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium

allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}    