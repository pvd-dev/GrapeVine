fileignoreconfig:
- filename: .github/workflows/python-test-app.yml
  checksum: 83ee6a8edd1960fff38d78451cc1e16fffd587092603805d2be6e0030f2ee86a
  ignore_detectors: [filename] 
version: ""

scopeconfig:
  - scope: yml

custom_severities:
- detector: Base64Content
  severity: medium
- detector: HexContent
  severity: medium

allowed_patterns:
- keyword
- aws-secret-access-key: ${{ secrets.AWS_SECRET_ACCESS_KEY }}
- aws-access-key-id: ${{ secrets.AWS_ACCESS_KEY_ID }}    